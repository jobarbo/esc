@import url('http://fonts.googleapis.com/css?family=Geo')

@font-face { font-family: 'pixelVector'; src: url('../fonts/kenvector_future_thin.ttf') format("truetype") /* Pretty Modern Browsers */ };
@font-face { font-family: 'smallest'; src: url('../fonts/work.ttf') format("truetype") /* Pretty Modern Browsers */ };
@font-face { font-family: 'pixelSmall'; src: url('../fonts/kenpixel_mini_square.ttf') format("truetype") /* Pretty Modern Browsers */ };
body
  margin auto

*
  margin 0
  padding 0

.hiddenText
  font-family pixelSmall 
  visibility hidden 
  height 0

